<div class="container">
		<div class="col-md-12"></div>
		<h2>Your eBay Shopping Cart</h2>

		
		<div class="panel col-md-10">

			<div class="row">
				<div class="panel col-md-8" ng-repeat="x in productsincart"
					style="background-color: white; border-color: black; margin: 10px;">
					<div class="panel col-md-12"
						style="background-color: #eeeeee; margin-top: 10px;">
						<div class="col-md-4">
						<div class="row">
							<a href="#" style="color: #00509d;"><b>{{x.name}}</b></a>
							</div>
							<div class="row">
							<table>
								<tr >
								<img src="/img/iphone.png" style="height:80px;width:60px">
								</tr>
								<tr>
									<td>Size:</td>
									 <td>{{x.size}}</td>
									
								</tr>
							</table>
							</div>
						</div>

						<div class="col-md-4">
							Quantity : <input type="number" style="width: 30px;" value={{x.quantity}}>
						</div>

						<div class="col-md-4" style="text-align: right">
							Price : <label>{{x.cost*x.quantity}}</label>
						</div>
						<a href='' style="float: right; color: #00509d;" id={{x.id}} onclick="removefromcart(this.id)">Remove</a>
					</div>
				</div>


				<div class="panel col-md-3"
					style="background-color: white; border-color: black; margin: 10px; padding-bottom: 20px;">
					<div class="col-md-12">
						<h4>Cart Summary</h4>
						<hr>
					</div>

					<div class="col-md-12">
						<h4>Total: ${{total}}</h4>
					</div>
					<div class="col-md-12">
						<button class="btn btn-primary" style="background-color: #00509d;" ng-click="checkout()">Proceed
							to checkout</button>
					</div>
					<div class="col-md-12" style="padding: 10px;">
						<img alt="" src="/img/eMBG_stacked.gif">
					</div>
					<div class="col-md-12" style="font-size: 12px;">Covers your
						purchase price plus original shipping on virtually all items. Get
						the item you ordered or get your money back.</div>
				</div>




			</div>

			<div class="row">
				<div class="col-md-3"></div>
				<div class="panel col-md-5"
					style="background-color: white; border-color: black; margin: 10px;">
					<div class="row" style="padding-top: 10px;">
						<div class="col-md-3"></div>
						<div class="col-md-3"></div>
						<div class="col-md-3" style="text-align: right">Subtotal :</div>
						<div class="col-md-3" style="text-align: left">${{total}}</div>
					</div>

					<div class="row">
						<div class="col-md-3"></div>
						<div class="col-md-3"></div>
						<div class="col-md-3" style="text-align: right">Shipping :</div>
						<div class="col-md-3" style="text-align: left">FREE</div>
					</div>

					<div class="row">
						<div class="col-md-3"></div>
						<div class="col-md-3"></div>
						<div class="col-md-3" style="text-align: right">
							<h4>Total :</h4>
						</div>
						<div class="col-md-3" style="text-align: left">
							<h4>${{total}}</h4>
						</div>
					</div>

					<div class="row" style="padding: 10px;">
						
						<div class="col-md-4">
							<button class="btn"
								style="background-color: #fafafa; border: 1px solid #aaa;"
							><a href="/homepage">Continue
								shopping</a></button>
						</div>
						<div class="col-md-1"></div>
						<div class="col-md-3">
							<button class="btn btn-primary"
								style="background-color: #00509d;">Proceed to checkout</button>
						</div>
					</div>

				</div>
			</div>
